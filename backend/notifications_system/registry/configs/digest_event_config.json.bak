{
  "user.activity_digest": {
    "event_key": "user.activity_digest",
    "channels": [
      "email"
    ],
    "interval": "weekly",
    "group_by": "user_id",
    "digest": {
      "merge_strategy": "list_append",
      "max_items": 20
    },
    "template": "emails/digests/activity_summary.html"
  },
